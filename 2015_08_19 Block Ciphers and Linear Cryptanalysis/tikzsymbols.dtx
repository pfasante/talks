\newcommand*\tikzsymbols@use@box[1]{%
  \expandafter\expandafter\expandafter\usebox%
  \expandafter\expandafter\expandafter{%
    \csname\current@tikzsymbols tikzsymbols@savebox@#1\endcsname}%
}
